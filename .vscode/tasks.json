{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: configure",
      "type": "cmake",
      "command": "configure",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "CMake: build",
      "type": "cmake",
      "command": "build",
      "dependsOn": "CMake: configure",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
