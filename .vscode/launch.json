{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug MyProgram",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/MyProgram.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/bin",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/raylib/w64devkit/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build",
      "logging": {
        "moduleLoad": false,
        "trace": false
      },
      "targetArchitecture": "x64"
    }
  ]
}
